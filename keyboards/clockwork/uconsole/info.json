{
    "manufacturer": "ClockworkPi",
    "keyboard_name": "uConsole",
    "url": "https://www.clockworkpi.com/uconsole",
    "maintainer": "neggles",
    "board": "STM32_F103_STM32DUINO",
    "bootloader": "stm32duino",
    "bootloader_instructions": "Keyboard will enter bootloader briefly on power-up, or can be forced by setting DIP switch 2 to ON.",
    "processor": "STM32F103",
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": false,
        "command": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "key_override": true,
        "nkro": false,
        "backlight": true,
        "virtser": true,
        "space_cadet": false,
        "grave_esc": false
    },
    "matrix_pins": {
        "cols": [ "C0", "C1", "C2", "C3", "C4", "C5", "C6", "C7" ],
        "rows": [ "NO_PIN", "NO_PIN", "A0", "A1", "A2", "A3", "A4", "A5", "A6", "A7" ],
        "ghost": true,
        "input_pressed_state": 0,
        "custom_lite": true
    },
    "usb": {
        "device_version": "1.0.0",
        "vid": "0x1EAF",
        "pid": "0x0024"
    },
    "backlight": {
        "driver": "pwm",
        "pin": "A8",
        "levels": 2,
        "on_state": 0
    },
    "bootmagic": {
        "matrix": [ 4, 2 ]
    },
    "build": {
        "debounce_type": "sym_defer_g",
        "lto": true
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [ 0, 0 ], "label": "Up", "x": 1.5, "y": 0, "w": 1 },
                { "matrix": [ 1, 5 ], "label": "MouseL", "x": 3.75, "y": 0 },
                { "matrix": [ 0, 7 ], "label": "JoyY", "x": 9.75, "y": 0 },
                { "matrix": [ 0, 6 ], "label": "JoyX", "x": 10.75, "y": 0 },

                { "matrix": [ 0, 2 ], "label": "Left", "x": 0.5, "y": 0.5 },
                { "matrix": [ 0, 1 ], "label": "Down", "x": 1.5, "y": 1, "w": 1 },
                { "matrix": [ 0, 3 ], "label": "Right", "x": 2.5, "y": 0.5 },
                { "matrix": [ 1, 7 ], "label": "MouseR", "x": 4.25, "y": 1 },
                { "matrix": [ 0, 5 ], "label": "JoyB", "x": 10.25, "y": 1 },
                { "matrix": [ 0, 4 ], "label": "JoyA", "x": 11.25, "y": 1 },

                { "matrix": [ 4, 2 ], "label": "Escape", "x": 0, "y": 2.5, "w": 1.5 },
                { "matrix": [ 2, 0 ], "label": "Select", "x": 1.75, "y": 2.5, "w": 1.25 },
